{
	"compilerOptions": {
		"composite": true,
		"lib": ["esnext"],
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": true,
		"strict": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"jsxImportSource": "@intrnl/jsx-to-string",
		"types": ["@cloudflare/workers-types"],
		"paths": {
			"~/*": ["./*"]
		}
	},
	"include": ["src"]
}
